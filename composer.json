{
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "4.4.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.4.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "*"
        }
      }
    }
  ],
  "require": {
    "wordpress/wordpress": "4.4.2",
    "fancyguy/webroot-installer": "*",
    "wpackagist-plugin/wordpress-seo": "*",
    "wpackagist-plugin/google-analytics-for-wordpress": "*",
    "wpackagist-plugin/google-sitemap-generator": "*",
    "wpackagist-theme/twentysixteen": "*"
  },
  "extra": {
    "installer-paths": {
      "app/themes/{$name}": ["type:wordpress-theme"],
      "app/plugins/{$name}": ["type:wordpress-plugin"]
    },
    "webroot-dir": "data",
    "webroot-package": "wordpress/wordpress"
  }
}
